        <activity android:name=".Vista.RegistrarLinea" />
        <activity android:name=".Vista.Registralado" />
        <activity android:name=".Vista.RegistrarMesa" />
        <activity android:name=".Vista.RegistrarMotivo" />
        <activity android:name=".Vista.RegistrarPersona" />
        <activity android:name=".Vista.RegistraUbicaciones" />
        <activity android:name=".Vista.MostrarUbicaciones"
